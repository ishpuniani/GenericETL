{
  "extract": {
    "type": "generic",
    "source": {
      "type": "http",
      "path": "https://www.propertypriceregister.ie/website/npsra/ppr/npsra-ppr.nsf/Downloads/PPR-ALL.zip/$FILE/PPR-ALL.zip",
      "unzip": true
    },
    "destination": {
      "type": "s3",
      "path": "resources/extract/ppr.csv"
    }
  },
  "transform": {
    "type": "csv",
    "mapping": "resources/transform_mapping.json",
    "source": {
      "type": "s3",
      "path": "resources/extract/ppr.csv"
    },
    "destination": {
      "type": "s3",
      "path": "resources/transform/ppr.csv"
    }
  },
  "load": {
    "type": "csv",
    "backup": true,
    "update": {"source": {
        "type": "s3",
        "path": "resources/transform/ppr.csv"
      }
    },
    "current": {
      "source": {
        "type": "s3",
        "path": "resources/load/ppr_current.csv"
      }
    },
    "destination": {
      "type": "s3",
      "path": "resources/load/ppr_current.csv"
    }
  }
}